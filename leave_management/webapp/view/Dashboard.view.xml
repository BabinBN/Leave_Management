<mvc:View controllerName="leavemanagement.controller.Dashboard"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f"
	height="100%">
	<App>
		<Page id="myPage" title="Side Navigation Example" enableScrolling="true">
